    {
      "Attributes": [
        {
          "Key": "ecer_name",
          "Type": 16,
          "Value": "Action needed: Transcript translated to English"
        },
        {
          "Key": "statecode",
          "Type": 4,
          "Value": {
            "Value": 0
          }
        },
        {
          "Key": "statuscode",
          "Type": 4,
